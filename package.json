{
  "name": "elu",
  "version": "1.0.4",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder --win --config.asar=false",
    "deploy": "electron-builder --win -p always --config.publish.releaseType=release"
  },
  "dependencies": {
    "electron-updater": "^6.6.2",
    "electron-log": "^5.0.1"
  },
  "devDependencies": {
    "electron": "^28.1.0",
    "electron-builder": "^26.0.12"
  },
  "build": {
    "appId": "com.elu.app",
    "productName": "Elu",
    "asar": false,
    "publish": {
      "provider": "github",
      "owner": "fpsParser",
      "repo": "Elu",
      "releaseType": "release"
    },
    "win": {
      "target": "nsis",
      "icon": "build/icon.ico"
    },
    "extraResources": [
      {
        "from": "node_modules/electron-updater",
        "to": "node_modules/electron-updater"
      }
    ]
  }
}